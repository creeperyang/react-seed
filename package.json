{
  "name": "react-seed",
  "version": "1.0.0",
  "description": "Seed project for React.js apps",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "babel-node --presets es2015,stage-0 --eval \"require('./tools/clean').default().catch(err => console.log(err.stack))\"",
    "prebuild": "npm run clean",
    "build": "babel-node --presets es2015,stage-0 --eval \"require('./tools/build').default().catch(err => console.log(err.stack))\"",
    "serve": "NODE_ENV=development babel-node --presets es2015,stage-0 --eval \"require('./tools/dev-server')\"",
    "start": "npm run clean && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creeperyang/react-seed.git"
  },
  "keywords": [
    "react",
    "webpack",
    "react-hot-reloader",
    "starter",
    "boilerplate",
    "babel"
  ],
  "author": "creeperyang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creeperyang/react-seed/issues"
  },
  "homepage": "https://github.com/creeperyang/react-seed#readme",
  "devDependencies": {
    "autoprefixer": "^6.2.3",
