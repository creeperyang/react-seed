    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.1",
    "node-sass": "^3.4.2",
    "opn": "^3.0.3",
    "postcss-loader": "^0.8.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "sass-loader": "^3.1.2",
